    <div class="uza-blog-area section-padding-80">
		<div class="container bootstrap snippet">
    <div class="row">
  		  <div class="col-sm-10"></div>
    	  <div class="col-sm-2"><a href="/users" class="pull-right"></a></div>
    </div>
    <div class="row Form">
  		  <div class="col-sm-3"><!--left col-->
            <div class="text-center">
                <img src="http://ssl.gstatic.com/accounts/ui/avatar_2x.png" class="avatar img-circle img-thumbnail" alt="avatar">

            </div></hr><!-- <br>

            <div class="panel panel-default">
				        <div class="panel-body"><center>
					           <a href="./employees/<%= username%>/profile/edit" class="btn uza-btn btn-2" data-animation="fadeInUp" data-delay="700ms">Edit Profile</a>
                               <button class="btn uza-btn btn-2 edit" id="edit_button">Create Profile</button>
					           <br>
			               </center>
                </div>
            </div> -->
          
        </div><!--/col-3-->
    	  <div class="col-sm-9">
            <ul class="nav nav-tabs">
                <li class="active"></li>
            </ul>
            <div class="tab-content">
                <div class="tab-pane active" id="home">
                    <form method="POST" action="/newBoss">
                    <div class="form-group">
                        <div class="col-xs-6">
                            <label for="username"><h4>Name<sup>*</sup></h4></label>
                            <input name="name" type="text" class="form-control" placeholder="Enter name"   required/>
                          </div>
                      </div>
                      <div class="form-group">
                          
                          <div class="col-xs-6">
                              <label for="username"><h4>Username<sup>*</sup></h4></label>
                              <input name="username" type="text" class="form-control" placeholder="Enter username"   required/>
                          </div>
                      </div>

                      <div class="form-group">
                          
                          <div class="col-xs-6">
                              <label for="designation"><h4>Designation<sup>*</sup></h4></label>
                              <input name="designation" type="text" class="form-control" placeholder="Enter designation"  required />
                          </div>
                      </div>
                      
          
                      <div class="form-group">
                          
                        <div class="col-xs-6">
                            <label for="phone"><h4>Office No.<sup>*</sup></h4></label>
                            <input name="phone" type="text" class="form-control" placeholder="Enter phone number"   required/>
                        </div>
                    </div>
        
                    <div class="form-group">
                        <div class="col-xs-6">
                           <label for="mobile"><h4>Mobile No.<sup>*</sup></h4></label>
                            <input name="mobile" type="text" class="form-control" placeholder="Enter mobile number"  required />
                        </div>
                    </div>
                    <div class="form-group">
                        
                        <div class="col-xs-6">
                            <label for="email"><h4>Email<sup>*</sup></h4></label>
                            <input name="email" type="text" class="form-control" placeholder="Enter email "  required />
                        </div>
                    </div>
                    <div class="form-group">
                      <div class="col-xs-6">
                         <label for="mobile"><h4>Gender<sup>*</sup></h4></label>
                          <input name="gender" type="text" class="form-control" placeholder="Enter gender "  required />
                      </div>
                  </div>
                    <div class="form-group">
                        
                        <div class="col-xs-6">
                            <label for="location"><h4>Address<sup>*</sup></h4></label>
                            <input name="address" type="text" class="form-control" placeholder="Enter address"   required/>
                        </div>
                    </div>
                    <div class="form-group">
                        
                      <div class="col-xs-6">
                          <label for="location"><h4>Nationality<sup>*</sup></h4></label>
                          <input name="nationality" type="text" class="form-control" placeholder="Enter nationality" required />
                      </div>
                  </div>
                  <div class="form-group">
                        
                      <div class="col-xs-6">
                          <label for="location"><h4>Date of Birth<sup>*</sup></h4></label>
                          <input name="dob" type="text" class="form-control" placeholder="Enter date of birth"  required />
                      </div>
                  </div>
                  <div class="form-group">
                        
                      <div class="col-xs-6">
                          <label for="location"><h4>Adhaar No.<sup>*</sup></h4></label>
                          <input name="adhaar" type="text" class="form-control" placeholder="Enter adhaar number" required/>
                      </div>
                  </div>
                  <div class="form-group">
                        
                      <div class="col-xs-6">
                          <label for="location"><h4>Account No.<sup>*</sup></h4></label>
                          <input name="account" type="text" class="form-control" placeholder="Enter account number" required/>
                      </div>
                  </div>
                  <div class="form-group">
                        
                      <div class="col-xs-6">
                          <label for="location"><h4>Joining Date<sup>*</sup></h4></label>
                          <input name="joining" type="text" class="form-control" placeholder="Enter date of joining" required/>
                      </div>
                  </div>
                  <div class="form-group">
                        
                      <div class="col-xs-6">
                          <label for="location"><h4>Marital Status</h4></label>
                          <input name="marital" type="text" class="form-control" placeholder="Enter marital status" required/>
                      </div>
                  </div>
                  <div class="form-group">
                        
                      <div class="col-xs-6">
                          <label for="location"><h4>Enter Password<sup>*</sup></h4></label>
                          <input name="password" type="password" class="form-control" placeholder="Enter password" required/>
                      </div>
                  </div>
                  <div class="form-group">
                        
                      <div class="col-xs-6">
                          <label for="location"><h4>Retype Password<sup>*</sup></h4></label>
                          <input name="conf_password" type="password" class="form-control" placeholder="Confirm password" required/>
                      </div>
                  </div>
                    <center><div class="fff"><button class="btn uza-btn btn-2 edit" id="edit_button">Add Boss</button></div></center>
                </form>
                <hr>
            </div><!--/tab-pane-->
        </div><!--/tab-pane-->

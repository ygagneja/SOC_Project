                                )
                        )
                    e.preventDefault();

                    var frm = $(this).closest("form");
                    var frm_id = frm.attr("id");
                    var frm_id_splitted = frm_id.split("_");
                    var frm_id_splitted_2 = frm_id_splitted[2];

                    var frm_serialized = frm.serialize();
                    $.ajax({
                        url: "/chatAppend",
                        method: "POST",
                        data: frm_serialized,
                        success: function (data) {

                        }
                    });
                }
            });
        });
    </script>
    <script type="text/javascript">
        function refreshAnnouncements() {
            var data = null;

            var xhr = new XMLHttpRequest();
            xhr.withCredentials = true;

            xhr.addEventListener("readystatechange", function () {
                if (this.readyState === 4) {

                    data = JSON.parse(this.responseText);
                    $("#remove").remove();
                    $("#announcements_list").append($("<div>").attr("id", "remove"))
                    for (var i = data.length - 1; i >= 0; i--) {
                        $("#remove")
                            .append(
                                $("<li>").attr("class", "border-bottom pb-3 mt-3")
                                    .append(
                                        $("<span>").attr("class", "meta text-uppercase")
                                            .append(
                                                $("<h5>").append($("<b>").append($("<font>").attr("color", "dodgerblue").text(data[i].date)))
                                            )
                                    )
                                    .append(
                                        $("<h4>").attr("class", "font-weight-bold mt-0").text(data[i].announcement)
                                    )
                            )
                    }
                }
            });

            xhr.open("GET", "/refreshAnnouncements");
            xhr.setRequestHeader("cache-control", "no-cache");

            xhr.send(data);
        }
        setInterval(refreshAnnouncements, 20000);

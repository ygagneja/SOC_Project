                                                        .attr('alt', 'User Avatar').attr('class', 'img-circle')
                                                )
                                        )
                                        .append(
                                            $("<div>").attr('class', 'chat-body clearfix')
                                                .append(
                                                    $("<div>").attr("class", "header")
                                                        .append(
                                                            $("<small>").attr('class', 'text-muted')
                                                                .append(
                                                                    $("<span>").attr('class', 'glyphicon glyphicon-time')
                                                                )
                                                                .text(data[i].time)
                                                        )
                                                        .append(
                                                            $("<strong>").attr("class", "pull-right primary-font").text(data[i].name)
                                                        )
                                                        .append(
                                                            $("<p>").text(data[i].message)
                                                        )
                                                )
                                        )
                                )
                        }
                        else {
                            $("#removeC")
                                .append(
                                    $("<li>").attr('class', 'left clearfix')
                                        .append(
                                            $("<span>").attr('class', 'chat-img pull-left')
                                                .append(
                                                    $("<img>").attr('src', 'http://placehold.it/50/55C1E7/fff&text=U')
                                                        .attr('alt', 'User Avatar').attr('class', 'img-circle')
                                                )
                                        )
                                        .append(
                                            $("<div>").attr('class', 'chat-body clearfix')
                                                .append(
                                                    $("<div>").attr("class", "header")
                                                        .append(
                                                            $("<strong>").attr("class", "primary-font").text(data[i].name)
                                                        )
                                                        .append(
                                                            $("<small>").attr("class", "pull-right text-muted")
                                                                .append(
                                                                    $("<span>").attr("class", "glyphicon glyphicon-time")
                                                                ).text(data[i].time)
                                                        )
                                                        .append(
                                                            $("<p>").text(data[i].message)
                                                        )
                                                )
                                        )
                                )
                        }
                    }
                }
            });

            xhr.open("GET", "/refreshMessages");
            xhr.setRequestHeader("cache-control", "no-cache");

            xhr.send(data);
        }
        setInterval(refreshMessages, 5000);
